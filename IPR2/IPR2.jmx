def status = prev.getResponseCode() ?: &quot;&quot;
def label = prev.getSampleLabel() ?: &quot;unknown&quot;

log.info(&quot;ðŸ’¬ status = ${status}, label = ${label}&quot;)

if (status ==~ /^\d{3}$/ &amp;&amp; status != &quot;200&quot;) {
    def body = &quot;custom_errors,transaction=${label},code=${status} count=1&quot;

    def url = new URL(&quot;http://localhost:8086/write?db=jmeter&quot;)
    def conn = url.openConnection()
    conn.setRequestMethod(&quot;POST&quot;)
    conn.doOutput = true

    def writer = new OutputStreamWriter(conn.outputStream)
    writer.write(body)
    writer.flush()
    writer.close()

    def resp = conn.getResponseCode()
    def msg = conn.getResponseMessage()

    log.info(&quot;ðŸš¨ Sent to Influx: &quot; + body)
    log.info(&quot;ðŸ“¡ Influx response: ${resp} ${msg}&quot;)
} else {
    log.info(&quot;ðŸ›‘ Not sending to Influx â€” bad or OK status: ${status}&quot;)
}</stringProp>
